+	res = usb_autopm_get_interface(dev->intf);
+	if (res < 0)
+		goto out_free;
+
+	mutex_lock(&dev->control);
+
+	res = ax88179_bind(dev);
+	if (res < 0)
+		goto out_free;
+
+	netif_carrier_off(netdev);
+	netif_start_queue(netdev);
+	/* Memory barrier
+	 */
+	smp_mb__before_atomic();
+	set_bit(WORK_ENABLE, &dev->flags);
+	/* Memory barrier
+	 */
+	smp_mb__after_atomic();
+
+	set_tx_qlen(dev);
